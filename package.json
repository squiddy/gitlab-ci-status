{
  "name": "gitlab-ci-status",
  "version": "1.0.0",
  "description": "Quick overview over gitlab CI pipelines",
  "main": "index.js",
  "repository": {},
  "author": "Reiner Gerecke",
  "license": "MIT",
  "scripts": {
    "start": "yarn run build:live",
    "build:live": "nodemon --exec ./node_modules/.bin/ts-node --inspect -- ./index.ts",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "express": "^4.15.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.4",
    "@types/express": "^4.0.36",
    "@types/jest": "^20.0.4",
    "@types/node": "^8.0.14",
    "jest": "^20.0.4",
    "nodemon": "^1.11.0",
    "prettier": "^1.5.3",
    "ts-jest": "^20.0.7",
    "ts-node": "^3.2.1",
    "typescript": "^2.4.2"
  }
}
